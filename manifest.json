{
  "manifest_version": 2,
  "name": "Expensify Downloader",
  "description": "Download expense reports selected by search on www.expensify.com",
  "version": "0.1",
  "homepage_url": "https://github.com/tumakha/expensify-downloader-chrome",
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon1024.png",
    "default_title": "Expensify Downloader"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://www.expensify.com/*"
  ],
  "icons": {
    "32": "icons/icon1024.png",
    "48": "icons/icon1024.png",
    "128": "icons/icon1024.png",
    "1024": "icons/icon1024.png"
  }
}